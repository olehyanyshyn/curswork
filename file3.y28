NAME _PROGRAM ; 
BODY DATA INTEGER16 _AAAAAAA , _BBBBBBB , _CYCLEAA , _CYCLEBB , _XVALUEE;
  GET ( _AAAAAAA )
  GET ( _BBBBBBB )

  FOR _BBBBBBB :> _CYCLEAA DOWNTO _AAAAAAA DO
    PUT ( _CYCLEAA * _CYCLEAA )

  0 :> _XVALUEE
  FOR _AAAAAAA :> _CYCLEAA DOWNTO 1 DO
    FOR _BBBBBBB :> _CYCLEBB DOWNTO 1 DO
      _XVALUEE ++ 1 :> _XVALUEE 
  PUT ( _XVALUEE )

END
